{
    "name": "rack-snippets",
    "displayName": "Rack snippets",
    "description": "Code snippets for VCV Rack module development",
    "version": "1.0.2",
    "publisher": "Lyqst",
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lyqst/vscode-rack-snippets"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "cpp",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "json",
                "path": "./snippets/snippets-json.code-snippets"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^1.99.0"
    },
    "scripts": {
        "deploy": "vsce publish"
    }
}
